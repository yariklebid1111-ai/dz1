result = []

def divider(a, b):
    if a < b:
        raise ValueError("a < b — не можна ділити!")
    if b > 100:
        raise IndexError("b > 100 — заборонене значення!")
    return a / b

data = {10: 2, 2: 5, "123": 4, 18: 0, (1,2): 15, 8: 4}

for key in data:
    try:
        res = divider(key, data[key])
        result.append(res)
    except Exception as error:
        print(f"Помилка для пари ({key}: {data[key]}) -> {error}")

print("Результат:", result)
